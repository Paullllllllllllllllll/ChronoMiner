{
  "name": "CulinaryPlacesEntries",
  "schema_version": "2.0",
  "type": "json_schema",
  "strict": true,
  "schema": {
    "type": "object",
    "properties": {
      "contains_no_content_of_requested_type": {
        "type": "boolean",
        "description": "Set to true if the input text contains no culinary places. Otherwise, set to false."
      },
      "entries": {
        "type": "array",
        "description": "List of places of culinary importance. If no places are found, return an empty array.",
        "items": {
          "type": "object",
          "properties": {
            "name_original": {
              "type": ["string", "null"],
              "description": "Place name exactly as found in the source (preserve spelling, capitalization, and diacritics)."
            },
            "name_modern_english": {
              "type": ["string", "null"],
              "description": "Modern English equivalent of the place name. Example: 'Firenze' â†’ 'Florence'. If no change is customary, repeat the original."
            },
            "place_type": {
              "type": "string",
              "enum": [
                "City",
                "Region",
                "Country",
                "Market",
                "Harbor",
                "Restaurant",
                "CulinarySchool",
                "Court",
                "MonasticCenter",
                "Inn",
                "Other",
                "Unknown"
              ],
              "description": "Best-fitting category for the place."
            },
            "country_modern": {
              "type": ["string", "null"],
              "description": "Modern country name in English. Example: 'Italy' or 'France'. Avoid ISO codes."
            },
            "period": {
              "type": "object",
              "description": "Active period or most relevant timeframe for the place in the given context.",
              "properties": {
                "start_year": {
                  "type": ["integer", "null"],
                  "description": "Earliest relevant year (four-digit Gregorian). Use null if unknown."
                },
                "end_year": {
                  "type": ["integer", "null"],
                  "description": "Latest relevant year (four-digit Gregorian). Use null if unknown."
                },
                "notation": {
                  "type": ["string", "null"],
                  "description": "Human-readable note about timing. Example: '18th c.' or 'opened 1896'."
                }
              },
              "required": ["start_year", "end_year", "notation"],
              "additionalProperties": false
            },
            "roles_in_culinary_ecosystem": {
              "type": "array",
              "description": "Functional roles this place played in food systems and culture. Select one or more.",
              "items": {
                "type": "string",
                "enum": [
                  "MarketCenter",
                  "ProductionArea",
                  "Harbor/Gateway",
                  "DistributionHub",
                  "Restaurant",
                  "CourtKitchen",
                  "MonasticKitchen",
                  "CulinarySchool",
                  "Inn",
                  "Other",
                  "Unknown"
                ],
                "description": "One role value from the controlled list."
              }
            },
            "associated_products": {
              "type": "array",
              "description": "Named products, dishes, or specialties linked to the place. Example: 'caciocavallo' or 'Argenteuil asparagus'.",
              "items": {
                "type": "string",
                "description": "Single product or dish name."
              }
            },
            "notable_establishments": {
              "type": "array",
              "description": "Named establishments (restaurants, hotels, schools) associated with the place.",
              "items": {
                "type": "string",
                "description": "Single establishment name. Example: 'La Tour d'Argent'."
              }
            },
            "associated_people": {
              "type": "array",
              "description": "Notable persons associated with this place.",
              "items": {
                "type": "object",
                "properties": {
                  "name_original": {
                    "type": ["string", "null"],
                    "description": "Person's name as found in source."
                  },
                  "name_modern_english": {
                    "type": ["string", "null"],
                    "description": "Modern English or standardized form for linkage."
                  },
                  "association_type": {
                    "type": "string",
                    "description": "Nature of association. Example: 'worked at' or 'founded'."
                  }
                },
                "required": ["name_original", "name_modern_english", "association_type"],
                "additionalProperties": false
              }
            },
            "notes": {
              "type": ["string", "null"],
              "description": "Additional context or clarifications. Use null if none."
            },
            "links": {
              "type": "array",
              "description": "Cross-references to related entities.",
              "items": {
                "type": "object",
                "properties": {
                  "entity_type": {
                    "type": "string",
                    "enum": ["Person", "Place", "Work", "Product", "Other"],
                    "description": "Type of entity being referenced."
                  },
                  "entity_label": {
                    "type": "string",
                    "description": "Name or label of the related entity."
                  },
                  "relationship": {
                    "type": "string",
                    "description": "Nature of the relationship."
                  }
                },
                "required": ["entity_type", "entity_label", "relationship"],
                "additionalProperties": false
              }
            }
          },
          "required": [
            "name_original",
            "name_modern_english",
            "place_type",
            "country_modern",
            "period",
            "roles_in_culinary_ecosystem",
            "associated_products",
            "notable_establishments",
            "associated_people",
            "notes",
            "links"
          ],
          "additionalProperties": false
        }
      }
    },
    "required": ["contains_no_content_of_requested_type", "entries"],
    "additionalProperties": false
  }
}