                      "anyOf": [
                        { "type": "string" },
                        { "type": "null" }
                      ],
                      "description": "Person's canonical name as it appears in the source (preserve spelling and diacritics)."
                    },
                    "modern_english": {
                      "anyOf": [
                        { "type": "string" },
                        { "type": "null" }
                      ],
                      "description": "Modern English or standardized form of the name for linkage. If no standardized form exists, repeat the original."
