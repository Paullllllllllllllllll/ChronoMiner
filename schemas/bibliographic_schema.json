{
  "name": "BibliographicEntries",
  "schema_version": "3.4",
  "type": "json_schema",
  "strict": true,
  "schema": {
    "type": "object",
    "properties": {
      "contains_no_content_of_requested_type": {
        "type": "boolean",
        "description": "Set to true if the input text contains no bibliographic entries. Otherwise, set to false."
      },
      "entries": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "full_title": {
              "type": "string",
              "description": "Complete official title in plain text, preserving archaic spelling."
            },
            "short_title": {
              "type": "string",
              "description": "A concise title or reference name in plain text. Example: 'Art of Cooking'."
            },
            "main_author": {
              "type": "string",
              "description": "Primary author name. Use 'Anonymous' if no author is attributed."
            },
            "culinary_focus": {
              "type": "array",
              "items": {
                "type": "string",
                "enum": [
                  "General",
                  "Baking and Pastry",
                  "Brewing and Distilling",
                  "Confectionery and Sweets",
                  "Preserving and Pickling",
                  "Medicinal and Health",
                  "Vegetarian and Vegan",
                  "Regional and Ethnic Cuisine",
                  "Dietary and Nutrition",
                  "Beverages and Drinks",
                  "Carving and Presentation",
                  "Wine and Viticulture",
                  "Meat and Poultry",
                  "Fish and Seafood",
                  "Herbs and Spices",
                  "Sauces and Condiments",
                  "Culinary Techniques",
                  "Culinary History",
                  "Culinary Arts Education",
                  "Agriculture and Viticulture",
                  "Household Management",
                  "Table Service and Etiquette",
                  "Food Adulteration and Safety",
                  "Unknown",
                  "Other"
                ],
                "description": "Culinary focus areas. Example: ['Baking and Pastry', 'Culinary Techniques']. Derived from the title if no further context can be found in the input text."
              },
              "description": "An array indicating the specific culinary areas the book focuses on."
            },
            "library_location": {
              "anyOf": [
                {
                  "type": "object",
                  "properties": {
                    "library_name": {
                      "anyOf": [
                        { "type": "string" },
                        { "type": "null" }
                      ],
                      "description": "Name of the library or archive holding the work. Example: 'Library of Congress' or 'Bibliothèque nationale'. Use null if not mentioned."
                    },
                    "library_city": {
                      "anyOf": [
                        { "type": "string" },
                        { "type": "null" }
                      ],
                      "description": "City where the library is located, in modern English. Example: 'Washington' or 'Paris'. Use null if not mentioned."
                    }
                  },
                  "required": ["library_name", "library_city"],
                  "additionalProperties": false
                },
                { "type": "null" }
              ],
              "description": "Physical location of the work. Use null if not mentioned."
            },
            "edition_info": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "year": {
                    "anyOf": [
                      { "type": "integer" },
                      { "type": "null" }
                    ],
                    "description": "The year of publication. Use an integer or null if unknown. Example: 1751."
                  },
                  "edition_number": {
                    "anyOf": [
                      { "type": "integer" },
                      { "type": "null" }
                    ],
                    "description": "Edition number (1,2,3...) or null if unknown."
                  },
                  "publication_locations": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "country": {
                          "anyOf": [
                            { "type": "string" },
                            { "type": "null" }
                          ],
                          "description": "Country of publication, modern English. Example: 'Italy'. Use null if unknown."
                        },
                        "city": {
                          "anyOf": [
                            { "type": "string" },
                            { "type": "null" }
                          ],
                          "description": "City of publication, modern English. Example: 'Rome'. Use null if unknown."
                        }
                      },
                      "required": ["country", "city"],
                      "additionalProperties": false
                    },
                    "description": "Array of publication locations for this edition. Multiple cities indicate co-publication."
                  },
                  "contributors": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "name": {
                          "anyOf": [
                            { "type": "string" },
                            { "type": "null" }
                          ],
                          "description": "Name of the contributor. Use null if unknown."
                        },
                        "role": {
                          "anyOf": [
                            {
                              "type": "string",
                              "enum": [
                                "Author",
                                "Editor",
                                "Translator",
                                "Compiler",
                                "Illustrator",
                                "Unknown",
                                "Other"
                              ]
                            },
                            { "type": "null" }
                          ],
                          "description": "Role of the contributor for this edition. Use null if unknown."
                        }
                      },
                      "required": ["name", "role"],
                      "additionalProperties": false
                    },
                    "description": "Additional contributors for this specific edition (editors, translators, etc.). Use empty array if none."
                  },
                  "publishers": {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "description": "Name of publisher or printer as recorded."
                    },
                    "description": "Publishers or printers for this edition. Use empty array if not specified."
                  },
                  "edition_category": {
                    "anyOf": [
                      {
                        "type": "string",
                        "enum": [
                          "Original",
                          "Reprint",
                          "Translation",
                          "Plagiarism",
                          "Modified Edition",
                          "Revised Edition",
                          "Enlarged Edition",
                          "Compilation",
                          "Facsimile",
                          "Critical Edition",
                          "Abridged Edition",
                          "Unknown",
                          "Other"
                        ]
                      },
                      { "type": "null" }
                    ],
                    "description": "The category of the edition. Use a listed value, or null if the source omits it. Example: 'Original'."
                  },
                  "short_note": {
                    "anyOf": [
                      { "type": "string" },
                      { "type": "null" }
                    ],
                    "description": "A brief note of any important information. Example: 'Second edition with added chapters.' or 'Revised and enlarged'."
                  },
                  "language": {
                    "anyOf": [
                      {
                        "type": "string",
                        "enum": [
                          "German",
                          "Spanish",
                          "English",
                          "French",
                          "Italian",
                          "Dutch",
                          "Latin",
                          "Portuguese",
                          "Danish",
                          "Swedish",
                          "Czech",
                          "Polish",
                          "Hungarian",
                          "Catalan",
                          "Unknown",
                          "Other"
                        ]
                      },
                      { "type": "null" }
                    ],
                    "description": "Language of this edition. Use a listed value, or null if not stated. Example: 'Italian'."
                  },
                  "translated_from": {
                    "anyOf": [
                      {
                        "type": "string",
                        "enum": [
                          "German",
                          "Spanish",
                          "English",
                          "French",
                          "Italian",
                          "Dutch",
                          "Latin",
                          "Portuguese",
                          "Danish",
                          "Swedish",
                          "Czech",
                          "Polish",
                          "Hungarian",
                          "Catalan",
                          "Unknown",
                          "Other"
                        ]
                      },
                      { "type": "null" }
                    ],
                    "description": "The source language from which this edition was translated. Use a listed value, or null if not a translation. Example: 'French'."
                  },
                  "format": {
                    "anyOf": [
                      {
                        "type": "string",
                        "enum": [
                          "Folio",
                          "Quarto",
                          "Octavo",
                          "Duodecimo",
                          "Sextodecimo",
                          "Pocket-Sized",
                          "Miniature",
                          "Unknown",
                          "Other"
                        ]
                      },
                      { "type": "null" }
                    ],
                    "description": "Physical format of this edition. Use a listed value, or null if not provided."
                  },
                  "pages": {
                    "anyOf": [
                      { "type": "integer" },
                      { "type": "null" }
                    ],
                    "description": "Total number of pages or leaf count for this edition, or null if unknown."
                  },
                  "has_illustrations": {
                    "anyOf": [
                      { "type": "boolean" },
                      { "type": "null" }
                    ],
                    "description": "True if this edition contains illustrations, plates, or engravings; false if it does not; null if unknown."
                  },
                  "dimensions": {
                    "anyOf": [
                      { "type": "string" },
                      { "type": "null" }
                    ],
                    "description": "Standardized dimensions in format 'height x width cm'. Example: '18.5 x 12.0 cm'. Use null if not specified."
                  },
                  "price_information": {
                    "anyOf": [
                      {
                        "type": "object",
                        "properties": {
                          "price": {
                            "anyOf": [
                              { "type": "number" },
                              { "type": "null" }
                            ],
                            "description": "Numeric price value. Example: 3.50. Use null if unknown."
                          },
                          "currency": {
                            "anyOf": [
                              { "type": "string" },
                              { "type": "null" }
                            ],
                            "description": "Currency abbreviation. Example: 'fr.' or '£'. Use null if unknown."
                          }
                        },
                        "required": ["price", "currency"],
                        "additionalProperties": false
                      },
                      { "type": "null" }
                    ],
                    "description": "Historical or catalog price for this edition. Use null if not mentioned."
                  }
                },
                "required": [
                  "year",
                  "edition_number",
                  "publication_locations",
                  "contributors",
                  "publishers",
                  "edition_category",
                  "short_note",
                  "language",
                  "translated_from",
                  "format",
                  "pages",
                  "has_illustrations",
                  "dimensions",
                  "price_information"
                ],
                "additionalProperties": false
              },
              "description": "Detailed information about each edition. The first edition should also be included here with its specific details."
            }
          },
          "required": [
            "full_title",
            "short_title",
            "main_author",
            "culinary_focus",
            "library_location",
            "edition_info"
          ],
          "additionalProperties": false
        }
      }
    },
    "required": ["contains_no_content_of_requested_type", "entries"],
    "additionalProperties": false
  }
}