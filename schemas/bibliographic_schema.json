{
  "name": "BibliographicEntries",
  "schema_version": "3.8",
  "type": "json_schema",
  "strict": true,
  "schema": {
    "type": "object",
    "properties": {
      "contains_no_content_of_requested_type": {
        "type": "boolean",
        "description": "Set to true if the input text contains no bibliographic entries. Otherwise, set to false."
      },
      "entries": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "full_title": {
              "type": "string",
              "description": "Complete official title in plain text, preserving archaic spelling."
            },
            "short_title": {
              "type": "string",
              "description": "A concise title or reference name in plain text. Example: 'Art of Cooking'."
            },
            "main_author": {
              "type": "string",
              "description": "Primary author name. Use 'Anonymous' if no author is attributed."
            },
            "short_note": {
              "anyOf": [
                { "type": "string" },
                { "type": "null" }
              ],
              "description": "A concise 3-10 sentence note with additional historiographical information (content, authors, historical context, etc.) about this work in modern english. Set to null if no such information is given."
                  },
            "library_abbreviation": {
              "anyOf": [
                { "type": "string" },
                { "type": "null" }
              ],
              "description": "Abbreviation of the holding library or archive of the first/main edition of the work. Use null if not mentioned. Example: 'UBA = Universiteitsbibliotheek Amsterdam'"
            },
            "volumes_overview": {
              "anyOf": [
                { "type": "string" },
                { "type": "null" }
              ],
              "description": "Brief summary of the work's volume structure (e.g., 'Three volumes: Vol. 1 (1645) original recipes; Vol. 2 (1783) expanded regional dishes; Vol. 3 (1978) facsimile reprint with modern annotations.'). Use null when the work is single-volume."
            },
            "volume_numbers": {
              "anyOf": [
                {
                  "type": "array",
                  "items": {
                    "anyOf": [
                      { "type": "integer" },
                      { "type": "null" }
                    ]
                  },
                  "description": "List of normalized volume numbers (integers)."
                },
                { "type": "null" }
              ],
              "description": "Explicit enumeration of volume numbers if the source lists them; use null for confirmed single-volume works."
            },
            "edition_info": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "year": {
                    "anyOf": [
                      { "type": "integer" },
                      { "type": "null" }
                    ],
                    "description": "The year of publication. Use an integer or null if unknown. Example: 1751."
                  },
                  "edition_number": {
                    "anyOf": [
                      { "type": "integer" },
                      { "type": "null" }
                    ],
                    "description": "Edition number (1,2,3...) or null if unknown."
                  },
                  "volume_numbers": {
                    "anyOf": [
                      {
                        "type": "array",
                        "items": {
                          "type": "integer"
                        }
                      },
                      { "type": "null" }
                    ],
                    "description": "Ordered list of volume numbers covered by this edition. Use null if the edition pertains to a single-volume work or volumes are not indicated."
                  },
                  "publication_locations": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "country": {
                          "anyOf": [
                            { "type": "string" },
                            { "type": "null" }
                          ],
                          "description": "Country of publication, modern English. Example: 'Italy'. Use null if not given; use 'Unknown' if you cannot determine the modern equivalent."
                        },
                        "city": {
                          "anyOf": [
                            { "type": "string" },
                            { "type": "null" }
                          ],
                          "description": "City of publication, modern English. Example: 'Rome'. Use null if given; use 'Unknown' if you cannot determine the modern equivalent."
                        }
                      },
                      "required": ["country", "city"],
                      "additionalProperties": false
                    },
                    "description": "Array of publication locations for this edition. Multiple cities indicate co-publication."
                  },
                  "contributors": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "name": {
                          "anyOf": [
                            { "type": "string" },
                            { "type": "null" }
                          ]
                        },
                        "role": {
                          "anyOf": [
                            {
                              "type": "string",
                              "enum": [
                                "Author",
                                "CoAuthor",
                                "Editor",
                                "Translator",
                                "Commentator",
                                "Annotator",
                                "Compiler",
                                "Contributor",
                                "Publisher",
                                "Printer",
                                "Illustrator",
                                "Cartographer",
                                "Engraver",
                                "Woodcutter",
                                "Lithographer",
                                "PrefaceWriter",
                                "IndexMaker",
                                "Dedicatee",
                                "Patron",
                                "Sponsor",
                                "Unknown",
                                "Other"
                              ]
                            },
                            { "type": "null" }
                          ],
                          "description": "Role of the contributor for this edition. Use null if unknown."
                        }
                      },
                      "required": ["name", "role"],
                      "additionalProperties": false
                    },
                    "description": "Additional contributors for this specific edition (editors, translators, etc.). Use empty array if none."
                  },
                  "publishers": {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "description": "Name of publisher or printer as recorded."
                    },
                    "description": "Publishers or printers for this edition. Use empty array if not specified."
                  },
                  "edition_category": {
                    "anyOf": [
                      {
                        "type": "string",
                        "enum": [
                          "Original",
                          "Reprint",
                          "Translation",
                          "Plagiarism",
                          "Modified Edition",
                          "Revised Edition",
                          "Enlarged Edition",
                          "Compilation",
                          "Facsimile",
                          "Critical Edition",
                          "Abridged Edition",
                          "Unknown",
                          "Other"
                        ]
                      },
                      { "type": "null" }
                    ],
                    "description": "The category of the edition. Use a listed value, or null if the source omits it. Example: 'Original'."
                  },
                  "language": {
                    "anyOf": [
                      {
                        "type": "string",
                        "enum": [
                          "German",
                          "Spanish",
                          "English",
                          "French",
                          "Italian",
                          "Dutch",
                          "Latin",
                          "Portuguese",
                          "Danish",
                          "Swedish",
                          "Norwegian",
                          "Finnish",
                          "Icelandic",
                          "Czech",
                          "Polish",
                          "Hungarian",
                          "Catalan",
                          "Greek",
                          "Russian",
                          "Ukrainian",
                          "Turkish",
                          "Arabic",
                          "Chinese",
                          "Japanese",
                          "Unknown",
                          "Other"
                        ]
                      },
                      { "type": "null" }
                    ],
                    "description": "Language of this edition. Use a listed value, or null if not stated. Example: 'Italian'."
                  },
                  "translated_from": {
                    "anyOf": [
                      {
                        "type": "string",
                        "enum": [
                          "German",
                          "Spanish",
                          "English",
                          "French",
                          "Italian",
                          "Dutch",
                          "Latin",
                          "Portuguese",
                          "Danish",
                          "Swedish",
                          "Norwegian",
                          "Finnish",
                          "Icelandic",
                          "Czech",
                          "Polish",
                          "Hungarian",
                          "Catalan",
                          "Greek",
                          "Russian",
                          "Ukrainian",
                          "Turkish",
                          "Arabic",
                          "Chinese",
                          "Japanese",
                          "Unknown",
                          "Other"
                        ]
                      },
                      { "type": "null" }
                    ],
                    "description": "The source language from which this edition was translated. Use a listed value, or null if not a translation. Example: 'French'."
                  },
                  "format": {
                    "anyOf": [
                      {
                        "type": "string",
                        "enum": [
                          "Folio",
                          "Quarto",
                          "Octavo",
                          "Duodecimo",
                          "Sextodecimo",
                          "Pocket-Sized",
                          "Miniature",
                          "Unknown",
                          "Other"
                        ]
                      },
                      { "type": "null" }
                    ],
                    "description": "Physical format of this edition. Use a listed value, or null if not provided."
                  },
                  "pages": {
                    "anyOf": [
                      { "type": "integer" },
                      { "type": "null" }
                    ],
                    "description": "Total number of pages or leaf count for this edition, or null if unknown."
                  },
                  "has_illustrations": {
                    "anyOf": [
                      { "type": "boolean" },
                      { "type": "null" }
                    ],
                    "description": "True if this edition contains illustrations, plates, or engravings; false if it does not; null if unknown."
                  },
                  "price_information": {
                    "anyOf": [
                      {
                        "type": "object",
                        "properties": {
                          "price": {
                            "anyOf": [
                              { "type": "number" },
                              { "type": "null" }
                            ],
                            "description": "Numeric price value. Example: 3.50. Use null if unknown."
                          },
                          "currency": {
                            "anyOf": [
                              { "type": "string" },
                              { "type": "null" }
                            ],
                            "description": "Currency abbreviation. Example: 'fr.' or '£'. Use null if unknown."
                          }
                        },
                        "required": ["price", "currency"],
                        "additionalProperties": false
                      },
                      { "type": "null" }
                    ],
                    "description": "Historical or catalog price for this edition. Use null if not mentioned."
                  }
                },
                "required": [
                  "year",
                  "edition_number",
                  "volume_numbers",
                  "publication_locations",
                  "contributors",
                  "publishers",
                  "edition_category",
                  "language",
                  "translated_from",
                  "format",
                  "pages",
                  "has_illustrations",
                  "price_information"
                ],
                "additionalProperties": false
              },
              "description": "Detailed information about each edition, including the first."
            }
          },
          "required": [
            "full_title",
            "short_title",
            "main_author",
            "short_note",
            "library_abbreviation",
            "volumes_overview",
            "volume_numbers",
            "edition_info"
          ],
          "additionalProperties": false
        }
      }
    },
    "required": ["contains_no_content_of_requested_type", "entries"],
    "additionalProperties": false
  }
}